{
  "name": "aquario",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/build-with-migrations.js",
    "start": "next start",
    "setup": "node scripts/setup.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint && npm run format:check && npm run type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "vitest",
    "test:integration:ui": "vitest --ui",
    "test:integration:watch": "vitest --watch",
    "test:all": "npm run test && npm run test:integration",
    "db:generate": "prisma generate",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:seed": "dotenv -e .env -- tsx prisma/seed.ts",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:reset": "dotenv -e .env -- prisma migrate reset",
    "release:patch": "npm version patch -m 'chore(release): %s'",
    "release:minor": "npm version minor -m 'chore(release): %s'",
    "release:major": "npm version major -m 'chore(release): %s'",
    "release:patch:push": "npm version patch -m 'chore(release): %s' && git push && git push --tags && gh release create $(git describe --tags --abbrev=0) --generate-notes",
    "release:minor:push": "npm version minor -m 'chore(release): %s' && git push && git push --tags && gh release create $(git describe --tags --abbrev=0) --generate-notes",
    "release:major:push": "npm version major -m 'chore(release): %s' && git push && git push --tags && gh release create $(git describe --tags --abbrev=0) --generate-notes",
    "postinstall": "prisma generate"
  },
